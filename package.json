{
  "name": "acacia-store",
  "version": "1.0.0",
  "description": "Extension marketplace server for hosting and managing serverless function + React component extensions",
  "module": "index.ts",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "bun run src/scripts/migrate.ts",
    "db:seed": "bun run src/scripts/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "hono": "^4.0.1",
    "drizzle-orm": "^0.29.3",
    "postgres": "^3.4.3",
    "drizzle-kit": "^0.20.12",
    "zod": "^3.22.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "tar": "^6.2.0",
    "node-fetch": "^3.3.2",
    "semver": "^7.5.4",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/tar": "^6.1.11",
    "@types/semver": "^7.5.6",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "keywords": [
    "extension",
    "marketplace",
    "serverless",
    "react",
    "drizzle",
    "api"
  ],
  "author": "",
  "license": "MIT"
}
